{
  "version": 2,
  "setting": {
    "time_tick_mode": "realtime",
    "sample_count_frame": 1024
  },
  "system_setting": {
    "file_io": {}
  },
  "node": {
    "_start_pin": {
      "type": "_start_pin"
    },
    "_dummy": {
      "type": "_dummy"
    },
    "input": {
      "type": "emitter-sine",
      "frequency": {
        "type": "a440",
        "value": "A4"
      },
      "intensity": 0.5,
      "range": {
        "start": 0.0,
        "length": 3.0
      },
      "sample_rate": 44100
    },
    "delay": {
      "type": "adapter-delay",
      "delay": 0.05
    },
    "mixer": {
      "type": "mix-stereo",
      "gain_0": {
        "type": "constant",
        "value": 0.707
      },
      "gain_1": {
        "type": "constant",
        "value": 0.707
      }
    },
    "output": {
      "type": "output-file",
      "format": {
        "type": "wav_lpcm16",
        "sample_rate": 44100
      },
      "file_name": "pseudo_stereo",
      "add_date_time": true
    }
  },
  "relation": [
    {
      "prev": {
        "node": "_start_pin",
        "pin": "out"
      },
      "next":{
        "node": "input",
        "pin": "in"
      }
    },
    {
      "prev": {
        "node": "input",
        "pin": "out"
      },
      "next": {
        "node": "delay",
        "pin": "in"
      }
    },
    {
      "prev": {
        "node": "input",
        "pin": "out"
      },
      "next": {
        "node": "mixer",
        "pin": "in_1"
      }
    },
    {
      "prev": {
        "node": "delay",
        "pin": "out"
      },
      "next": {
        "node": "mixer",
        "pin": "in_2"
      }
    },
    {
      "prev": {
        "node": "mixer",
        "pin": "out"
      },
      "next": {
        "node": "output",
        "pin": "in"
      }
    }
  ]
}